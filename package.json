    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "lint": "eslint './src/server/**/*.ts' && cd src/client && yarn lint",
    "postinstall": "cd src/client && yarn install --frozen-lockfile",
    "test": "gulp test",
    "test:unit": "jest",
    "tsc": "tsc"
  },
  "dependencies": {
